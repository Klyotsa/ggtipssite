    "gsap": "^3.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scroll": "^1.9.3",
    "styled-components": "^6.1.17",
    "styled-icons": "^10.47.1"
  },
  "devDependencies": {
    "@styled-icons/material": "^10.47.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
